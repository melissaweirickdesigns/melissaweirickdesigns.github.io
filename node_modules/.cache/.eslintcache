\"T8sA4laONCJsfRyct\")\n      .then((response) => {\n        console.log(\"Email successfully sent!\", response.status, response.text);\n        setMessage(\"Your message has been sent!\");\n      })\n      .catch((error) => {\n        console.error(\"Error sending email:\", error);\n        setMessage(\n          \"Error sending email: Please call (801) 386-6272 for assistance\"\n        );\n      });\n  };\n\n  const submitHandler = (event) => {\n    event.preventDefault();\n    if (\n      !formdata.name ||\n      !formdata.email ||\n      !formdata.subject ||\n      !formdata.message\n    ) {\n      setError(true);\n      setMessage(\"All fields are required\");\n    } else {\n      setError(false);\n      sendEmail(\"template_7f7o9i8\", {\n        name: formdata.name,\n        email: formdata.email,\n        subject: formdata.subject,\n        message: formdata.message\n      }, event);\n      setFormdata({\n        name: \"\",\n        email: \"\",\n        subject: \"\",\n        message: \"\",\n      });\n    }\n  };\n\n  const handleChange = (event) => {\n    setFormdata({\n      ...formdata,\n      [event.target.name]: event.target.value,\n    });\n  };\n\n  const numberFormatter = (number) => {\n    const phnNumber = number;\n    return phnNumber;\n  };\n\n  const handleAlerts = () => {\n    if (error && message) {\n      return <div className=\"alert alert-danger mt-4\">{message}</div>;\n    } else if (!error && message) {\n      return <div className=\"alert alert-success mt-4\">{message}</div>;\n    } else {\n      return null;\n    }\n  };\n\n  useEffect(() => {\n    const script = document.createElement(\"script\");\n    script.src = \"https://cdn.emailjs.com/sdk/2.6.4/email.min.js\";\n    script.onload = () => {\n      emailjs.init(\"T8sA4laONCJsfRyct\");\n      setScriptLoaded(true);\n    };\n    document.body.appendChild(script);\n\n    setTimeout(() => {\n      setPhoneNumbers([\"+1 (801) 386-6272\"]);\n      setEmailAddress([\"melissaweirickdesigns@gmail.com\"]);\n      setAddress(\"Woods Cross, UT 84087 USA\");\n    }, 1000);\n\n    return () => {\n      document.body.removeChild(script);\n    };\n  }, []);\n  \n\n  return (\n    <Layout>\n      <Helmet>\n        <title>Contact - Chester React Personal Portfolio Template</title>\n        <meta\n          name=\"description\"\n          content=\"Chester React Personal Portfolio Template Contact Page\"\n        />\n      </Helmet>\n      <Suspense fallback={<Spinner />}>\n        <div className=\"mi-contact-area mi-section mi-padding-top mi-padding-bottom\">\n          <div className=\"container\">\n            <Sectiontitle title=\"Contact Me\" />\n            <div className=\"row\">\n              <div className=\"col-lg-6\">\n                <div className=\"mi-contact-formwrapper\">\n                  <h4>Get In Touch</h4>\n                  <form\n                    action=\"#\"\n                    className=\"mi-form mi-contact-form\"\n                    onSubmit={submitHandler}\n                  >\n                    <div className=\"mi-form-field\">\n                      <label htmlFor=\"contact-form-name\">\n                        Enter your name*\n                      </label>\n                      <input\n                        onChange={handleChange}\n                        type=\"text\"\n                        name=\"name\"\n                        id=\"contact-form-name\"\n                        value={formdata.name}\n                      />\n                    </div>\n                    <div className=\"mi-form-field\">\n                      <label htmlFor=\"contact-form-email\">\n                        Enter your email*\n                      </label>\n                      <input\n                        onChange={handleChange}\n                        type=\"text\"\n                        name=\"email\"\n                        id=\"contact-form-email\"\n                        value={formdata.email}\n                      />\n                    </div>\n                    <div className=\"mi-form-field\">\n                      <label htmlFor=\"contact-form-subject\">\n                        Enter your subject*\n                      </label>\n                      <input\n                        onChange={handleChange}\n                        type=\"text\"\n                        name=\"subject\"\n                        id=\"contact-form-subject\"\n                        value={formdata.subject}\n                      />\n                    </div>\n                    <div className=\"mi-form-field\">\n                      <label htmlFor=\"contact-form-message\">\n                        Enter your Message*\n                      </label>\n                      <textarea\n                        onChange={handleChange}\n                        name=\"message\"\n                        id=\"contact-form-message\"\n                        cols=\"30\"\n                        rows=\"6\"\n                        value={formdata.message}\n                      ></textarea>\n                    </div>\n                    <div className=\"mi-form-field\">\n                      <button className=\"mi-button\" type=\"submit\">\n                        Send Mail\n                      </button>\n                    </div>\n                  </form>\n                  {handleAlerts()}\n                </div>\n              </div>\n              <div className=\"col-lg-6\">\n                <div className=\"mi-contact-info\">\n                  {!phoneNumbers ? null : (\n                    <div className=\"mi-contact-infoblock\">\n                      <span className=\"mi-contact-infoblock-icon\">\n                        <Icon.Phone />\n                      </span>\n                      <div className=\"mi-contact-infoblock-content\">\n                        <h6>Phone</h6>\n                        {phoneNumbers.map((phoneNumber) => (\n                          <p key={phoneNumber}>\n                            <a href={numberFormatter(phoneNumber)}>\n                              {phoneNumber}\n                            </a>\n                          </p>\n                        ))}\n                      </div>\n                    </div>\n                  )}\n                  {!emailAddress ? null : (\n                    <div className=\"mi-contact-infoblock\">\n                      <span className=\"mi-contact-infoblock-icon\">\n                        <Icon.Mail />\n                      </span>\n                      <div className=\"mi-contact-infoblock-content\">\n                        <h6>Email</h6>\n                        {emailAddress.map((email) => (\n                          <p key={email}>\n                            <a href={`mailto:${email}`}>{email}</a>\n                          </p>\n                        ))}\n                      </div>\n                    </div>\n                  )}\n                  {!phoneNumbers ? null : (\n                    <div className=\"mi-contact-infoblock\">\n                      <span className=\"mi-contact-infoblock-icon\">\n                        <Icon.MapPin />\n                      </span>\n                      <div className=\"mi-contact-infoblock-content\">\n                        <h6>Address</h6>\n                        <p>{address}</p>\n                      </div>\n                    </div>\n                  )}\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </Suspense>\n    </Layout>\n  );\n}\n\nexport default Contact;\n","/home/melissaweirickdesigns/melissa-weirick-portfolio/src/pages/Portfolios.js",[],[],"/home/melissaweirickdesigns/melissa-weirick-portfolio/src/pages/Notfound.js",[],[],"/home/melissaweirickdesigns/melissa-weirick-portfolio/src/pages/Resumes.js",[],[],"/home/melissaweirickdesigns/melissa-weirick-portfolio/src/pages/About.js",[],[],"/home/melissaweirickdesigns/melissa-weiric